{
    "manifest_version": 3,
    "name": "Not ChatGPT At All",
    "version": "1.0",
    "description": "use chatgpt but don't notice no one",
      "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
    "content_scripts": [
      {
        "matches": ["https://chat.openai.com/*"],
        "js": ["contentScript.js"]
      }
    ],
    "permissions": ["activeTab"] 
     
  }
  